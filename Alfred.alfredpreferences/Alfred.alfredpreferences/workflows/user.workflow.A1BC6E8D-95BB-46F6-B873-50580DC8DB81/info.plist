				<key>script</key>
				<string>LOCAL=$(ifconfig | grep -A 1 "en" | grep broadcast | cut -d " " -f 2)
EXTERNAL=$(curl ip.cn | grep -i "ip" | cut -d " " -f 3)

cat &lt;&lt;EOF
&lt;?xml version="1.0"?&gt;
&lt;items&gt;
EOF

LOCAL_RESULT_COUNT=1
LOCAL_ARR=($(echo $LOCAL | tr "\\n" $IFS))
for value in ${LOCAL_ARR[@]}; do
cat &lt;&lt;EOF
&lt;item uid="localip_$LOCAL_RESULT_COUNT" arg="$value"&gt;
&lt;title&gt;Local IP$LOCAL_RESULT_COUNT: $value&lt;/title&gt;
&lt;subtitle&gt;Press Enter to paste, or Cmd+C to copy&lt;/subtitle&gt;
&lt;icon&gt;icon.png&lt;/icon&gt;
&lt;/item&gt;
EOF
    LOCAL_RESULT_COUNT=`expr $LOCAL_RESULT_COUNT + 1`
done

cat &lt;&lt;EOF
&lt;item uid="externalip" arg="$EXTERNAL"&gt;
&lt;title&gt;External IP: $EXTERNAL&lt;/title&gt;
&lt;subtitle&gt;Press Enter to paste, or Cmd+C to copy&lt;/subtitle&gt;
&lt;icon&gt;icon.png&lt;/icon&gt;
&lt;/item&gt;
&lt;/items&gt;
EOF
</string>
